{
  "version": 2,
  "framework": null,
  "buildCommand": null,
  "installCommand": null,
  "rewrites": [
    {
      "source": "/",
      "destination": "/demo-switcher.html"
    },
    {
      "source": "/demo3",
      "destination": "/Octave Design Build - Demo3/html/index.html"
    },
    {
      "source": "/demo4",
      "destination": "/Octave Design Build - Demo4/html/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(js|css|jpg|jpeg|png|gif|svg|woff|woff2|ttf|eot))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
